/**
 * <b>Class VFC20_ContractDelete </b>: a controller for the corresponding Visual force page
 * @author Eric Wartelle
 * @version 1.0
 */
public with sharing class VFC20_ContractDelete {
	//Attributs
	/**
 	 * <b>standardController </b>: will store the curent standard controller
	 */
	private ApexPages.StandardController standardController;

	//Constructors
	/**
	 * <b>Constructor with one argument</b>
	 * @param standardController (ApexPages.StandardController) : 
	 */
	public VFC20_ContractDelete(ApexPages.StandardController standardController) {
		this.standardController = standardController;
	}

	//Methods
	/**
	 * <b>Static Methode canDelete</b>: test if the current user can delete this contract or not
	 * @param currentContract (Contract__c) : the contract we want to delete
	 * @return true or false
	 */
	private static Boolean canDelete(Contract__c currentContract){
		// System.debug call for debug log reason
		System.debug(LoggingLevel.INFO, '## VFC20_ContractDelete.canDelete - Start');
		Boolean contractDeletionFlag = DeletionPermissionFlag__c.getInstance(UserInfo.getUserId()).ContractDelete__c;

		// System.debug call for debug log reason
		System.debug(LoggingLevel.INFO, '## VFC20_ContractDelete.canDelete - End');
		return contractDeletionFlag;
	}

	/**
	 * <b>Method deleteContract </b>: called by the corresponding visualforce page
	 * @return PageReference : always null.
	 */
	public PageReference deleteContract(){
		//Local variable declaration
		Contract__c currentContract = (Contract__c) standardController.getRecord();

		//System.debug call for debug log purpose
		System.debug(LoggingLevel.INFO, '## VFC20_ContractDelete.deleteContract - Start');

		//Checking if we can delete the current contract
		if(canDelete(currentContract)){
			try {
				//Try to delete the contract
                delete currentContract;
            } catch (Exception e) {
            	//Error while deleting the contract
                ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.ERROR, e.getMessage()));
                return null;
            }
            //The contract was deleted
            ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.CONFIRM, 'Your contract has been deleted.'));
            return null;
		}
		//The user don't have the right to delete the contract
		ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.ERROR, 'You don\'t have the right to delete that contract. You must used the Cancelled status instead.'));

		//System.debug call for debug log purpose
		System.debug(LoggingLevel.INFO, '## VFC20_ContractDelete.deleteContract - End');
        return null;
	}


